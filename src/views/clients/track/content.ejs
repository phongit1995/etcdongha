<div class="content-page">
    <!-- Start content -->
    <div class="content">
        <div class="container-fluid">

            <div class="row">
                <div class="col-12">
                    <div class="page-title-box">
                        <h4 class="page-title float-left">Danh Sách Vượt Trạm</h4>

                        <ol class="breadcrumb float-right">
                            <li class="breadcrumb-item"><a href="#">Quản Lý</a></li>
                            <li class="breadcrumb-item"><a href="#">Vượt Trạm</a></li>
                            
                        </ol>
                        <div class="clearfix"></div>
                    </div>
                </div>
            </div>
            <div class="row">
                    <div class="col-12">
                            <div class="card-box table-responsive text-center">
                            <div class="panel-heading">
                                    <p><i class="glyphicon glyphicon-search"></i> Tra cứu thông tin <span class="search-toggle hide"><span class="glyphicon glyphicon-plus"></span>
                                        <span class="search-toggle"><span class="glyphicon glyphicon-minus"></span></span>
                                        </span>
                                    </p>
                                    <div style="margin-top: 10px;" class="seach-form">
                                        <div class="row">

                                            <div class="input-group col-md-4">
                                                <div class="input-group-prepend">
                                                    <div class="input-group-text"><strong>Biển số:</strong></div>
                                                </div>
                                                <input id="LicensePlatesSearch" type="text" class="form-control" placeholder="VD:74A99999">
                                            </div>
                                            <div class="col-md-4">
                                                <div class="input-group">
                                                    <div class="input-group-prepend">
                                                    <span class="input-group-text"><strong>Từ ngày:</strong></span>
                                                    </div>
                                                    <input id="DateStartSearch" type="text" class="form-control dateinput flatpickr-input"  readonly="readonly">
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="input-group">
                                                    <div class="input-group-prepend">
                                                    <span class="input-group-text"><strong>Đến ngày:</strong></span>
                                                    </div>
                                                    <input id="DateEndSearch" type="text" class="form-control dateinput flatpickr-input"  readonly="readonly">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row" style="margin-top:20px">
                                            <div class="col-md-8">
                                                <div class="message-search"></div>
                                            </div>
                                            <div class="col-md-4">
                                                <button type="button" class="btn btn-success waves-effect btn-search-form "><i class="fa fa-search"></i> Tìm Kiếm</button>
                                                <button class="btn btn-success" type="button" id="addTrack" data-toggle="modal" data-target="#addForm"><i class="fa fa-plus"></i> Thêm xe</button>
                                                <button type="button" class="btn btn-primary btn-clear-search"><i class="fa fa-spinner"></i> Làm lại</button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                </div>
                    </div>
                </div>
            <!-- end row -->
          
            <div class="row">
                <div class="col-12">
                    <div class="card-box table-responsive text-center">
                            <h4 class="text-uppercase  font-18">Danh sách vượt trạm:</h4>
                            <div class="border m-b-30"></div>
                        <table id="datatable" class="table table-bordered">
                            <thead>
                            <tr>
                                <th>ID</th>
                                <th>User</th>
                                <th>Biển Số Xe</th>
                                <th>Tên Khách Hàng</th>
                                <th>Làn</th>
                                <th>Thời Gian</th>
                                <th>Mệnh Giá</th>
                                <th>Ghi Chú</th>
                                <th>Thao Tác</th>
                            </tr>
                            </thead>
                            <tbody>
                            <% listTrack.forEach((item)=>{ %>
                                <tr>
                                    <td><%=item.TrackId %></td>
                                    <td><%=item.UserName %></td>
                                    <td><%=item.LicensePlates %></td>
                                    <td><%=item.NameCustomer %></td>
                                    <td><%=item.Lane %></td>
                                    <td><%=moment(item.TrackTime).tz("Asia/Bangkok").format("DD-MM-YYYY HH:mm")  %></td>

                                   
                                    <td><%=item.FeeNumbers %></td>
                                    <td><%=item.Notes %></td>
                                    <td>
                                        <div class="btn btn-group-xs">
                                        <% if(user.Role<3) {%>
                                                <button type="button" class="edit btn btn-success" data-id= <%=item.TrackId %> ><i class="fa fa-pencil"></i> Sửa</button>
                                                <button type="button" class="del btn btn-danger deleteTrack" data-id= <%=item.TrackId %> ><i class="fa fa-trash"></i> Xóa</button>
                                        <% }else if(user.Id==item.Id){ %>
                                        <button type="button" class="edit btn btn-success"  data-id= <%=item.TrackId %> ><i class="fa fa-pencil"></i> Sửa</button>
                                        <%} %>
                                        </div>
                                    </td>
                                </tr>
                            <%}) %>
                       
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        <!-- end row -->
            <!-- end row -->


        </div> <!-- container -->

    </div> <!-- content -->

    <footer class="footer text-right">
        2019 - 2022 © Etc đông hà. - Coder By Thành
    </footer>

</div>
