<div id="editForm" class="modal fade in" role="dialog" style="display: none; padding-right: 15px;">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">×</button>
                <h4 class="modal-title">Cập nhật thông tin </h4>
            </div>
            <div class="modal-body" id="editor-body">
                <form id="updateTrack">
                    <p><b>BIỂN SỐ XE</b>:
                        <input type="text" class="form-control" id="LicensePlatesEdit" name="LicensePlatesEdit" required="true" >
                    </p>
                    <p><b>TÊN KHÁCH HÀNG</b>:
                        <input type="text" class="form-control" id="NameCustomerEdit" name="NameCustomerEdit" required="true" value="Đang cập nhật">
                    </p>
                    <p><b>LÀN</b>
                        <br>
                        <select class="form-control" id="LaneEdit" name="LaneEdit">
                            <% listLane.forEach((item)=>{ %>
                                <option value="<%=item.LaneID%>"><%=item.LaneName%></option>
                           <% }) %>
                        </select>
                    </p>
                    <p><b>Ngày tháng</b>:
                        <input type="text" class="datetimepicker form-control flatpickr-input" id="TrackTimeEdit" name="TrackTimeEdit" required="true" readonly="readonly">
                    </p>
                    <p><b>GHI CHÚ</b>
                        <br>
                        <textarea class="form-control" id="NotesEdit" name="NotesEdit">Vượt trạm</textarea>
                    </p>
                    <p><b>MỆNH GIÁ</b>
                        <br>
                        <select class="form-control" id="FeeEdit" name="FeeEdit">
                            <% listFee.forEach(item=>{ %>
                                <option value="<%=item.IdFee %>"> <%=item.FeeNumbers%></option>
                            <% }) %>
                        </select>
                    </p>
                    <input type="hidden" id="idTrackEdit" name="idTrackEdit" >
                    <button class="btn btn-primary " id="UpdateTrack" type="button"><i class="fa fa-save"></i> Update</button>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Đóng</button>
            </div>
        </div>
    </div>
</div>



<div id="addForm" class="modal fade" role="dialog" style="display: none;">
        <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">×</button>
                    <h4 class="modal-title"><font color="green">Thêm thông tin</font></h4>
                </div>
                <div class="modal-body">
                    <form id="addTrackForm" action="javascript:return false;" method="POST">
                        <p><b>BIỂN SỐ XE</b>:
                            <input type="text" class="form-control" id="LicensePlates" name="trackNo" required="true" value="">
                            <!-- <div class="btn btn-search"><i class="fas fa-search"></i> </div> -->
                        </p>
                        <p><b>TÊN KHÁCH HÀNG</b>:
                            <input type="text" class="form-control" id="NameCustomers" name="Name" required="true" value="Đang cập nhật">
                        </p>
                        <p><b>LÀN</b>
                            <br>
                            <select class="form-control" id="Lane" name="Lane">
                                <% listLane.forEach((item)=>{ %>
                                    <option value="<%=item.LaneID%>"><%=item.LaneName%></option>
                               <% }) %>
                            </select>
                        </p>
                        <p><b>GHI CHÚ</b>
                            <br>
                            <textarea class="form-control" id="Notes" name="Notes" required="true">Vượt trạm</textarea>
                        </p>
                        <p><b>MỆNH GIÁ</b>
                            <br>
                            <select class="form-control" id="Fee" name="Fee">
                                <% listFee.forEach(item=>{ %>
                                    <option value="<%=item.IdFee %>"> <%=item.FeeNumbers%></option>
                                <% }) %>
                        
                            </select>
                        </p>
                        <p><b>Ngày tháng</b>:
                            <input type="text" class="datetimepicker form-control flatpickr-input datetimeinput " id="TrackTime" name="trackDate" required="true" value="" readonly="readonly">
                        </p>
                        <button class="btn btn-success add-track"><i class="fa fa-plus"></i> Lưu</button>
                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Đóng</button>
                </div>
            </div>
        </div>
    </div>
